@foreach (var entry in Entries)
{
    <Entry Placeholder="Expression" @bind-Text="@entry.Value" HorizontalOptions="XF.LayoutOptions.CenterAndExpand"/>
}
<Button Text="New expression" OnClick="@AddEntryCallback" HorizontalOptions="XF.LayoutOptions.Center" />

@using ServiceHealthChecker.DB.Models;
@code {

    [Parameter]
    public IEnumerable<StringColumn> Entries { get; set; }

    [Parameter]
    public EventCallback AddEntryCallback { get; set; }
}
