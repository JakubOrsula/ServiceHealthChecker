@foreach (var entry in Entries)
{
    <Entry Placeholder="Expression" 
           @bind-Text="@entry.Value" 
           ClearButtonVisibility="XF.ClearButtonVisibility.WhileEditing" 
           HorizontalOptions="XF.LayoutOptions.FillAndExpand"/>
}
<Button Text="New expression" OnClick="@AddEntryCallback"  
        HorizontalOptions="XF.LayoutOptions.Center" 
        StyleClass="btn btn-outline-secondary" />

@using ServiceHealthChecker.DB.Models;
@code {

    [Parameter]
    public IEnumerable<StringColumn> Entries { get; set; }

    [Parameter]
    public EventCallback AddEntryCallback { get; set; }
}
