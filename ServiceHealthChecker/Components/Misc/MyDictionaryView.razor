@foreach (var entry in Entries)
{
    <StackLayout Orientation="XF.StackOrientation.Horizontal">
        <Entry Placeholder="Key" @bind-Text="@entry.Key" HorizontalOptions="XF.LayoutOptions.StartAndExpand"/>
        <Entry Placeholder="Value" @bind-Text="@entry.Value" HorizontalOptions="XF.LayoutOptions.StartAndExpand"/>
    </StackLayout>
}
<Button Text="New pair" OnClick="@AddEntryCallback" HorizontalOptions="XF.LayoutOptions.Center"/>


@using ServiceHealthChecker.DB.Models;
@code {

    [Parameter]
    public IEnumerable<KeyValueColumn> Entries { get; set; }

    [Parameter]
    public EventCallback AddEntryCallback { get; set; }
}
